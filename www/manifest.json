{
	"_version": "1.7.0",
	"sap.app": {
		"id": "SAPUI5ExampleSAPUI5Example",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"operators_alias": {
				"uri": "model/Operators.json",
				"type": "JSON"
			},
			"plants_alias": {
				"uri": "model/PlantInformation.json",
				"type": "JSON"
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "SAPUI5ExampleSAPUI5Example.view.Main",
			"type": "XML"
		},
		"routing": {
	        "config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "SAPUI5ExampleSAPUI5Example.view",
				"controlId": "app",
				"controlAggregation": "pages"
		     },
			"routes": [
			
				{
				    "name": "Main",
					"pattern": "",
					"target":"Main"				
				},
				{
				    
					"name": "Plants",
					"pattern": "Main",
					"target": "Plants"
				
				}
			],
			"targets": {
				"Main": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Main"			
				},
				"Plants": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Plants"				
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "SAPUI5ExampleSAPUI5Example.i18n.i18n"
				}
			},
			"oPlantInfo": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "plants_alias"
			},
			"operators": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "operators_alias"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}